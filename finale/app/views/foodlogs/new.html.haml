= form_for Foodlog.new do |f|
  %h3 please enter your food here
  = f.label :date
  = f.date_field :customdate
  = f.label :food
  = f.text_field :food, :id => "Food"
  = f.submit "Add Food"
- if @foodlogs.any?
  - @foodlogs.each do |f|
    %h2= f.customdate
    %h2= f.food
    .delete
    = button_to 'Delete', foodlog_path(f), method: :delete
- else
  %h1 No Food Logged Yet
